{
  "name": "amitsn-blog-client",
  "homepage": "https://www.amitsn.com/",
  "version": "0.20.5",
  "private": true,
  "dependencies": {
    "@aws-sdk/shared-ini-file-loader": "^3.54.1",
    "@fortawesome/fontawesome-svg-core": "^6.1.0",
    "@fortawesome/free-solid-svg-icons": "^6.1.0",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "ajv": "^7.2.4",
    "ajv-errors": "^2.0.1",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^4.0.1",
    "aws-amplify": "^6.6.7",
    "axios": "^1.8.2",
    "create-react-class": "^15.7.0",
    "debug": "^4.3.4",
    "disqus-react": "^1.1.2",
    "highlight.js": "^11.5.0",
    "moment": "^2.29.4",
    "path-browserify": "^1.0.1",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-addons-shallow-compare": "^15.6.3",
    "react-autosize-textarea": "^7.1.0",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^16.14.0",
    "react-fontawesome": "^1.7.1",
    "react-ga": "^3.3.0",
    "react-helmet-async": "^2.0.5",
    "react-loading-skeleton": "^2.1.1",
    "react-lowlight": "^3.0.0",
    "react-markdown": "^8.0.0",
    "react-moment": "^1.1.1",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.2.0",
    "react-router-sitemap": "^1.2.0",
    "remove-markdown": "^0.3.0",
    "typescript": "^4.6.2"
  },
  "overrides": {
    "json5": ">=2.2.2",
    "semver": ">=7.5.2",
    "postcss": ">=8.4.31",
    "nth-check": ">=2.0.1",
    "serialize-javascript": ">=6.0.2"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "sitemap": "node src/sitemap/sitemap-generator.js",
    "clearprerender": "aws dynamodb delete-table --table-name AmitsnBlogPrerender || true",
    "createprerender": "aws dynamodb create-table --attribute-definitions AttributeName=url,AttributeType=S --table-name AmitsnBlogPrerender --key-schema AttributeName=url,KeyType=HASH --billing-mode PAY_PER_REQUEST",
    "incrementversion": "if [[ $(npm pkg get version) =~ '9\"'$ ]]; then npm version minor --no-git-tag-version; else npm version patch --no-git-tag-version; fi",
    "predeploy": "npm run sitemap && npm run clearprerender && npm run incrementversion && npm run build && npm run createprerender",
    "deploy": "aws s3 sync dist/ s3://amitsn-blog-client --delete",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E3EJ272XE26PWO --paths '/*'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@fortawesome/fontawesome-free": "^6.1.0",
    "@vitejs/plugin-react": "^4.5.1",
    "vite": "^6.3.5"
  }
}
